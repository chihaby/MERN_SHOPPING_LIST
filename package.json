  "bugs": {
    "url": "https://github.com/chihaby/MERN_SHOPPING_LIST/issues"
  },
  "homepage": "https://github.com/chihaby/MERN_SHOPPING_LIST#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "mongoose": "^5.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
}
